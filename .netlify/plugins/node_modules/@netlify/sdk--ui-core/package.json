{
  "name": "@netlify/sdk--ui-core",
  "version": "2.0.1",
  "author": "Netlify, Inc.",
  "description": "A framework-agnostic toolkit for building UI extensions on top of the Netlify platform.",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./mocks": {
      "types": "./dist/mocks.d.ts",
      "default": "./dist/mocks.js"
    }
  },
  "browserslist": {
    "production": [
      "last 2 Chrome versions",
      "Safari >= 15",
      "last 2 Edge versions",
      "last 2 Firefox versions",
      "Firefox ESR",
      "> 1%",
      "not IE 11",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "event-iterator": "^2.0.0",
    "iter-tools-es": "^7.5.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "eslint": "^9.39.1",
    "fast-check": "^4.3.0",
    "happy-dom": "^20.0.10",
    "prettier": "3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10",
    "@netlify/sdk--eslint-config": "0.0.0",
    "@netlify/sdk--typescript-config": "0.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch --preserveWatchOutput",
    "clean": "rm -rf dist/ *.tsbuildinfo",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  }
}