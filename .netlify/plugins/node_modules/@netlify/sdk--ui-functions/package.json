{
  "name": "@netlify/sdk--ui-functions",
  "version": "2.0.3",
  "author": "Netlify, Inc.",
  "description": "A toolkit for building Netlify Functions for use by UI extensions.",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./trpc": {
      "types": "./dist/trpc/main.d.ts",
      "default": "./dist/trpc/main.js"
    },
    "./trpc/unstable": {
      "types": "./dist/trpc/unstable.d.ts",
      "default": "./dist/trpc/unstable.js"
    }
  },
  "dependencies": {
    "@netlify/types": "^2.3.0",
    "zod": "^4.1.12",
    "@netlify/sdk--extension-api-client": "3.0.3"
  },
  "devDependencies": {
    "@trpc/server": "^11.7.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^18.19.130",
    "eslint": "^9.39.1",
    "prettier": "3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10",
    "@netlify/sdk--eslint-config": "0.0.0",
    "@netlify/sdk--typescript-config": "0.0.0"
  },
  "peerDependencies": {
    "@trpc/server": ">= 11 || >= 11.0.0-rc.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch --preserveWatchOutput",
    "clean": "rm -rf dist/ *.tsbuildinfo",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  }
}